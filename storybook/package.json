{
  "name": "@cin7/storybook",
  "version": "0.1.0",
  "private": true,
  "description": "Interactive component documentation for Cin7 Design System Library",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build -o storybook-static",
    "preview": "npx http-server storybook-static",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "perf:check": "node scripts/performance-manager.js",
    "perf:emergency-fix": "node scripts/emergency-fix.js",
    "perf:restore": "node scripts/emergency-fix.js restore",
    "dev:minimal": "STORYBOOK_CONFIG_PATH=main.minimal.ts storybook dev -p 6008",
    "build:minimal": "STORYBOOK_CONFIG_PATH=main.minimal.ts storybook build -o storybook-static-minimal"
  },
  "dependencies": {
    "@cin7/core": "workspace:*",
    "@cin7/design-tokens": "workspace:*",
    "@cin7/highcharts-adapter": "workspace:*",
    "@shopify/polaris": "^13.9.5",
    "@shopify/polaris-icons": "^9.3.1",
    "@shopify/polaris-tokens": "^9.4.2",
    "highcharts": "^12.4.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "selenium-webdriver": "^4.38.0"
  },
  "devDependencies": {
    "@storybook/addon-controls": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-storybook": "^0.10.1",
    "storybook": "^8.6.14",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
