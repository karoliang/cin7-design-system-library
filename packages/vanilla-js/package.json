{
  "name": "@cin7/vanilla-js",
  "version": "0.1.0",
  "description": "Vanilla JavaScript utilities for lightweight UI interactions in Cin7 DSL",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "browser": "./dist/browser.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "browser": "./dist/browser.js"
    },
    "./dom": {
      "types": "./dist/dom/index.d.ts",
      "import": "./dist/dom/index.mjs",
      "require": "./dist/dom/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.mjs",
      "require": "./dist/events/index.js"
    },
    "./animations": {
      "types": "./dist/animations/index.d.ts",
      "import": "./dist/animations/index.mjs",
      "require": "./dist/animations/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vitest"
  },
  "dependencies": {
    "@cin7/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jsdom": "^23.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}